{
    "manifest_version": 2,
    "name": "Clipsey",
    "version": "0.1",
    "browser_action": {
        "default_popup": "src/interface.html",
        "default_icon": "src/icons/favicon.jpg",
        "default_title": "Clipsey"
    },
    "permissions": ["activeTab"],

    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "src/js/shader.js"
            ],
            "run_at": "document_end"
        }
    ]
}