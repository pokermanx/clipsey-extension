{
    "manifest_version": 2,
    "name": "Clipsey",
    "version": "0.1",

    "browser_action": {
        "default_popup": "src/interface.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "src/content.js"
            ]
        }
    ]
}